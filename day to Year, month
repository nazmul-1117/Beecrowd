#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base :: sync_with_stdio(false);
    cin.tie(NULL);
    
    int n, x;
    cin>>n;
    x = n;
    
    cout<<n/365<<" ano(s)\n";
    x -= (n/365) * 365;
    cout<<x/30<<" mes(es)\n";
    x -= (x/30) * 30;
    cout<<x<<" dia(s)"<<endl;
    
    return 0;
}
